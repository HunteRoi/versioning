	"devDependencies": {
		"@faker-js/faker": "^6.0.0-alpha.7",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.21",
		"jest": "^27.5.1",
		"jest-cli": "^27.5.1",
